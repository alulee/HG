@model IList<FamilyMemberOverviewModel>
@{
    //Layout = "~/Views/Shared/_ColumnsTwo.cshtml";

    //title
    Html.AddTitleParts(T("PageTitle.FamilyMemberOverview").Text);
}
@using HGenealogy.Models.HGFamilyMembers
<div class="page category-page">

    <div class="page-title">
        <h1>FamilyMemberOverview</h1>
    </div>

    <div class="page-body">

        @if (Model.Count > 0)
        {
            <div class="product-grid">
                <div class="item-grid">
                    @foreach (var familymember in Model)
                    {
                        <div class="item-box">
                            @Html.Partial("_FamilyMemberBox", familymember)
                        </div>
                    }
                </div>
            </div>
        }
    </div>
</div>
