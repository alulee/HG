@model IList<FamilyMemberOverviewModel>

@using HGenealogy.Models.HGFamilyMembers;
@if (Model.Count > 0)
{
    <div class="related-products-grid product-grid">
        <div class="title">
            <strong>@T("其它相關家族成員")</strong>
        </div>
        <div class="item-grid">
            @foreach (var item in Model)
            {
                <div class="item-box">@Html.Partial("_FamilyMemberBox", item)</div>
            }
        </div>
    </div>
}